{
  "threshold": 3,
  "reporters": ["html", "json", "console"],
  "output": "./reports/jscpd/",
  "gitignore": true,
  "ignore": [
    "**/node_modules/**",
    "**/dist/**",
    "**/build/**",
    "**/*.test.ts",
    "**/*.test.tsx",
    "**/types.ts"
  ],
  "format": ["typescript", "javascript", "tsx", "jsx"],
  "min-lines": 3,
  "max-lines": 500,
  "min-tokens": 50,
  "mode": "mild",
  "silent": false,
  "blame": true,
  "cache": true,
  "absolute": true
}